{
  "name": "darcyx/yii2-gta",
  "description": "The Tpl extension for the Yii framework",
  "keywords": [
    "yii2",
    "template",
    "renderer"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.0"
  },
  "authors": [
    {
      "name": "darcy",
      "email": "darcyonw@163.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "yii\\gta\\": "src"
    },
    "files": [
      "src/Helps/functions.php"
    ]
  },
  "minimum-stability": "stable"
}
